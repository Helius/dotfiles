URxvt.termName: rxvt
URxvt.inheritPixmap: true
URxvt.imLocale: ru_RU.UTF-8
URxvt.urlLauncher: firefox
URxvt.fading: 20%
URxvt.shading: 40
!URxvt.blurRadius: 10

URxvt.perl-ext-common: selection,searchable-scrollback,readline,tabbed_new
URxvt.saveLines: 99999
URxvt.mouseWheelScrollPage: false

URxvt.font: xft:terminus-10: pixelsize=12: autohint=true: antialias=true

URxvt.scrollstyle: plain
URxvt.scrollBar_right: true
URxvt.scrollBar: true
URxvt.jumpScroll: true

! ======= plugin setup =======
URxvt.perl-lib: /home/eugene/.urxvt/
URxvt.perl-ext: selection,mark-yank-urls,urxvt_timestamp
! url yank
URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
URxvt.underlineURLs: true
URxvt.urlLauncher: firefox

! timestamps
URxvt.keysym.M-l: perl:urxvt_timestamp:activate_mark_mode

! tabbed-new
URxvt.tabbed_new.tabren-bg: 3
URxvt.tabbed_new.tabdiv-fg: 8
URxvt.tabbed_new.tabbar-fg: 8
URxvt.tabbed_new.tabbar-bg: 0
URxvt.tabbed_new.tabsel-fg: 1
URxvt.tabbed_new.tabsel-bg: 8
URxvt.tabbed_new.tab-fg:    0
URxvt.tabbed_new.tab-bg:    8


URxvt.foreground:#eeeeee
URxvt.background:#101010
URxvt.transparent: true

*background: rgb:1a/1a/1a
*foreground: rgb:aa/aa/aa

!black
*color0:     rgb:00/00/00
*color8:     rgb:66/66/66
!red
*color1:     rgb:9e/18/28
*color9:     rgb:bc/57/66
!green
*color2:     rgb:00/88/00
*color10:    rgb:61/a1/71
!brown/yellow
*color3:     rgb:d2/bb/4b
*color11:    rgb:e7/8b/52
!blue
*color4:     rgb:51/61/91
*color12:    rgb:50/85/af
!magenta
*color5:     rgb:96/3c/59
*color13:    rgb:a9/7a/99
!cyan
*color6:     rgb:41/81/79
*color14:    rgb:6b/a4/a4
!white
*color7:     rgb:ee/ee/ee
*color15:    rgb:30/30/30
